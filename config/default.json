{
  "services": {
    "aiAgents": {
      "enabled": true,
      "port": 3001,
      "maxAgents": 100,
      "defaultModel": "gpt-4",
      "apiTimeout": 30000
    },
    "gameServer": {
      "enabled": true,
      "port": 3002,
      "maxPlayers": 100,
      "tickRate": 60,
      "maxRooms": 50,
      "roomTimeout": 3600000
    },
    "cloudStorage": {
      "enabled": true,
      "port": 3003,
      "provider": "aws",
      "maxFileSize": 52428800,
      "allowedTypes": [
        "image/png",
        "image/jpeg",
        "image/gif",
        "model/gltf-binary",
        "application/octet-stream"
      ]
    },
    "puterDatabase": {
      "enabled": true,
      "connectionTimeout": 10000,
      "poolSize": 10
    }
  },
  "security": {
    "rateLimit": {
      "enabled": true,
      "windowMs": 900000,
      "maxRequests": 100
    },
    "cors": {
      "enabled": true,
      "origins": ["*"]
    }
  },
  "logging": {
    "level": "info",
    "format": "json",
    "destination": "stdout"
  }
}
