{
  "services": {
    "aiAgents": {
      "maxAgents": 1000
    },
    "gameServer": {
      "maxPlayers": 500,
      "maxRooms": 200
    }
  },
  "security": {
    "rateLimit": {
      "enabled": true,
      "windowMs": 900000,
      "maxRequests": 1000
    },
    "cors": {
      "origins": ["https://yourdomain.com"]
    }
  },
  "logging": {
    "level": "warn",
    "destination": "file"
  }
}
