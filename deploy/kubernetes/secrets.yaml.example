apiVersion: v1
kind: Secret
metadata:
  name: gdevelop-secrets
  namespace: gdevelop-assistant
type: Opaque
stringData:
  OPENAI_API_KEY: "your-openai-api-key-here"
  PUTER_DB_USER: "admin"
  PUTER_DB_PASSWORD: "your-secure-password-here"
  AWS_ACCESS_KEY_ID: "your-aws-access-key-here"
  AWS_SECRET_ACCESS_KEY: "your-aws-secret-key-here"
  AWS_S3_BUCKET: "gdevelop-assets"
