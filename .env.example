# Server Configuration
NODE_ENV=development
PORT=3000

# AI Agents Configuration
AI_AGENT_PORT=3001
OPENAI_API_KEY=your_openai_api_key_here
AI_AGENT_MODEL=gpt-4

# Game Server Configuration
GAME_SERVER_PORT=3002
GAME_SERVER_MAX_PLAYERS=100
GAME_SERVER_TICK_RATE=60

# Puter Database Configuration
PUTER_DB_HOST=localhost
PUTER_DB_PORT=27017
PUTER_DB_NAME=gdevelop_assistant
PUTER_DB_USER=admin
PUTER_DB_PASSWORD=your_db_password_here
PUTER_DB_AUTH_SOURCE=admin

# Cloud Storage Configuration
CLOUD_STORAGE_PROVIDER=aws
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
AWS_S3_BUCKET=gdevelop-assets
CLOUD_STORAGE_PATH=game-assets

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# API Configuration
API_RATE_LIMIT=100
API_RATE_WINDOW=900000
